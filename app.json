{
  "expo": {
    "name": "PrepNotion",
    "slug": "PrepNotion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/Landing.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "scheme":"PrepNotion",
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "PrepNotion accesses your camera to let you take profile pictures and share them with your friends."
        }
      ]
    ]
  }
}
